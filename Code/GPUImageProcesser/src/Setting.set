
#待处理文件路径
Source=H:\Image
#处理结果保存路径
Output=D:\TestImages
#文件类型筛选
Filter=.jpg|.png|.bmp|.gif
#指定操作的CL代码
Program=src\UnAlignedDoubleLinearScale.cl
#CL代码入口函数
Entry=UnAlignedDoubleLinearScale
#启用输入输出缓冲队列
IOQueue=true
#启用显存输入缓冲
DeviceInBuffer=false
#启用显存输出缓冲
DeviceOutBuffer=false
#输入缓冲大小(单位：个文件对象)
InBufferQueue=200
#输出缓冲大小(单位：个文件对象)
OutBufferQueue=200
#输出数据控制
#输出通道限制[1,3,4]
OutputChannel=3
#输出大小限制（Width*Height）
OutputSize=4096
#使输出与源目录结构一致
CopyDirStruct=true
